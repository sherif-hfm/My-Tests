<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="ITLS">
                    <reliableSession enabled="true" />
                    <security mode="None" />
                </binding>
                <binding name="ITLSWEB">
                    <reliableSession enabled="true" />
                    <security mode="None" />
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8020/TLS/TLSService.svc"
                binding="wsHttpBinding" bindingConfiguration="ITLS" contract="TlsWcf.ITLS"
                name="ITLS" />
            <endpoint address="http://localhost:9030/RoutingService.svc/tls"
                binding="wsHttpBinding" bindingConfiguration="ITLSWEB" contract="TlsWcf.ITLSWeb"
                name="ITLSWEB" />
        </client>
    </system.serviceModel>
  <system.net>
    <defaultProxy useDefaultCredentials="true">
      <proxy usesystemdefault="True" bypassonlocal="True"   />
    </defaultProxy>
  </system.net>
</configuration>